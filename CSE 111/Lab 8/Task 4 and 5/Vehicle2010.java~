public class Vehicle2010 {
        public int x = 0;
        public int y = 0;
        public void moveUpperRight() {
                ++y;
                ++x;
        }
        public void moveUpperLeft() {
                ++y;
                --x;
        }
        public void moveLowerRight() {
                ++x;
                --y;
        }
        public void moveLowerLeft() {
                --x;
                --y;
        }
        public String toString() {
                return String.format("(%d, %d)", x, y);
        }
 }